{
  "manifest_version": 3,
  "name": "Cookie Cloud",
  "version": "1.0.0",
  "description": "Cookie Cloud Chrome Extensions",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "icons/logo64.png",
      "128": "icons/logo128.png",
      "192": "icons/logo192.png",
      "256": "icons/logo256.png",
      "512": "icons/logo512.png"
    }
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "incognito": "not_allowed"
}