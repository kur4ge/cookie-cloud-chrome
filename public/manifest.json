{
  "manifest_version": 3,
  "name": "Demo",
  "version": "1.0.0",
  "description": "Demo Chrome Extensions",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "192": "icons/logo192.png",
      "512": "icons/logo512.png"
    }
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "incognito": "not_allowed"
}